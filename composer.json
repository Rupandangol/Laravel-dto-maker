{
    "name": "rupandangol/laravel-class-maker",
    "description": "Laravel helper to create Class like service class, enum , trait, dto and so on",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rupandangol\\LaravelClassMaker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "rupan dangol"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9 || ^10 || ^11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rupandangol\\LaravelClassMaker\\ClassMakerServiceProvider"
            ]
        }
    }
}
